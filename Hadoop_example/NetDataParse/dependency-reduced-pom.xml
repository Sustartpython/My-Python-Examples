<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>VIP.DataAnalysis</groupId>
  <artifactId>NetDataParse</artifactId>
  <name>NetDataParse</name>
  <version>0.1</version>
  <url>http://maven.apache.org</url>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.3.2</version>
        <configuration>
          <target>1.7</target>
          <source>1.7</source>
          <encoding>UTF-8</encoding>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.3</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer />
                <transformer>
                  <mainClass>simple/Main</mainClass>
                </transformer>
              </transformers>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>VipRepositoryGroup</id>
      <name>Vip Repository Group</name>
      <url>http://192.168.30.55:8081/nexus/content/repositories/snapshots/</url>
    </repository>
    <repository>
      <id>snapshots</id>
      <name>Snapshots</name>
      <url>http://192.168.30.55:8081/nexus/content/groups/VipRepositoryGroup/</url>
    </repository>
    <repository>
      <id>thirdparty</id>
      <name>thirdparty</name>
      <url>http://192.168.30.55:8081/nexus/content/repositories/thirdparty</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.12</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>hamcrest-core</artifactId>
          <groupId>org.hamcrest</groupId>
        </exclusion>
      </exclusions>
    </dependency>
  </dependencies>
  <distributionManagement>
    <repository>
      <id>snapshots</id>
      <name>Snapshots</name>
      <url>http://192.168.30.55:8081/nexus/content/repositories/snapshots/</url>
    </repository>
  </distributionManagement>
  <properties>
    <hbase.version>0.94.10-hadoop-2.6.0-cdh5.10.0-cqvip</hbase.version>
    <hadoop.version>2.6.0-cdh5.10.0-cqvip</hadoop.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>

